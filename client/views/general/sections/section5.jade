  .row
    h2 Say Hello!
    .col-xs-6.form
      form.contactus
        .form-group
          input#name.form-control(type='text', name='name', placeholder='Your name', autofocus=true, ng-model='visitor.name', required)
        .form-group
          input#email.form-control(type='email', name='email', placeholder='Your Email', autofocus=true, ng-model='visitor.email', required)
        .form-group
          textarea#textarea.form-control(type='text', name='text', placeholder='Your message', autofocus=true, ng-model='visitor.message', required)
        .form-group
          button.btn.btn-default(type='submit', ng-click='submit(visitor)') Submit
